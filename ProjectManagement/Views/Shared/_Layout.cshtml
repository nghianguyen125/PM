    <link rel="apple-touch-icon" href="images/icon152.png" sizes="152x152">
    <link rel="apple-touch-icon" href="images/icon180.png" sizes="180x180">
    <!-- END Icons -->
    <!-- Stylesheets -->
    <!-- Bootstrap is included in its original form, unaltered -->
