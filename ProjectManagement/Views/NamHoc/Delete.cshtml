@model ProjectManagement.Models.NamHoc

@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<section class="content-header">
    <h1>Năm Học / Học Kỳ</h1>
</section>
<section class="content">
    <div class="box box-info">
        <div class="box-header with-border">
            <h3 class="box-title">Bạn có chắc muốn xóa?</h3>
        </div>
        <dl class="dl-horizontal">
            <dt>
                Mã Năm học:
            </dt>
            <dd>
                @Html.DisplayFor(model => model.NamHocHocKyId)
            </dd>
            <dt>
                Chi tiết:
            </dt>
            <dd>
                @Html.DisplayFor(model => model.TenNamHocHocKy)
            </dd>
            <dt>
                Ngày Bắt đầu:
            </dt>
            <dd>
                @Html.DisplayFor(model => model.TuNgay)
            </dd>
            <dt>
                Ngày Kết thúc:
            </dt>
            <dd>
                @Html.DisplayFor(model => model.DenNgay)
            </dd>
            <dt>
                Root:
            </dt>
            <dd>
                @Html.DisplayFor(model => model.NamHocHocKyIdRoot)
            </dd>
        </dl>

        @using (Html.BeginForm())
        {
            @Html.AntiForgeryToken()
            <div class="box-footer">
                @*<button type="submit" class="btn btn-default">Cancel</button>*@
                @Html.ActionLink("Back to List", "Index", null, new { @class = "btn btn-default" })
                <input type="submit" value="Delete" class="btn btn-primary pull-right" />
            </div>
        }
    </div>
</section>

